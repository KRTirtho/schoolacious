{
    "name": "@veschool/titumir",
    "version": "0.1.0",
    "description": "",
    "author": "KR Tirtho",
    "private": true,
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
        "start": "nest start",
        "dev:watch": "nodemon -q -e js -w dist -x 'node dist/src/main.js'",
        "check-types": "nodemon -q -e ts -w ./src -x 'tsc --noEmit'",
        "dev": "concurrently -n 'esbuild,tsc' -p '{name}' -c 'bgYellow.black.bold,bgBlue.black.bold' --kill-others -i --default-input-target=2 'node esbuild.config.js' 'npm run check-types' 'npm run dev:watch'",
        "debug:watch": "nodemon -q -e js -w dist -x  'node --inspect=0.0.0.0:9229 dist/src/main.js'",
        "debug": "concurrently -n 'esbuild,tsc' -p '{name}' -c 'bgYellow.black.bold,bgBlue.black.bold' --kill-others -i --default-input-target=2 'node esbuild.config.js' 'npm run check-types' 'npm run debug:watch'",
        "prod": "node dist/main",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test jest --config jest.config.ts ",
        "test:watch": "cross-env NODE_ENV=test jest --config jest.config.ts --watch",
        "test:cov": "cross-env NODE_ENV=test jest --config jest.config.ts --coverage",
        "test:debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.ts --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest.e2e-config.ts",
        "test:e2e-debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.e2e-config.ts --runInBand"
    },
    "dependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/platform-socket.io": "^7.6.17",
        "@nestjs/schedule": "^0.4.3",
        "@nestjs/swagger": "^4.8.1",
        "@nestjs/throttler": "^1.1.4",
        "@nestjs/typeorm": "^7.1.5",
        "@nestjs/websockets": "^7.6.17",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "csurf": "^1.11.0",
        "date-fns": "^2.22.1",
        "dotenv": "^8.2.0",
        "helmet": "^4.4.1",
        "lodash": "^4.17.21",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.6",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.32",
        "uuid": "^8.3.2",
        "validator": "^13.6.0"
    },
    "devDependencies": {
        "@anatine/esbuild-decorators": "^0.2.11",
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.15",
        "@types/bcrypt": "^3.0.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/cron": "^1.7.2",
        "@types/csurf": "^1.11.1",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/lodash": "^4.14.169",
        "@types/node": "^14.14.36",
        "@types/passport-jwt": "^3.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/pg": "^7.14.11",
        "@types/socket.io": "^2.1.13",
        "@types/supertest": "^2.0.10",
        "@types/uuid": "^8.3.0",
        "concurrently": "^6.2.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.12.9",
        "glob": "^7.1.7",
        "jest": "^26.6.3",
        "node-mocks-http": "^1.10.1",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0"
    }
}
