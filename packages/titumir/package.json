{
    "name": "@schoolacious/titumir",
    "version": "0.1.0",
    "description": "",
    "author": "KR Tirtho",
    "private": true,
    "license": "BSD-4-Clause",
    "scripts": {
        "build": "nest build",
        "build:fast": "node esbuild.config.js",
        "check-types": "watchexec -e ts -w src 'tsc --noEmit'",
        "debug": "concurrently -n 'esbuild,tsc' -p '{name}' -c 'bgYellow.black.bold,bgBlue.black.bold' --kill-others -i --default-input-target=2 'node esbuild.config.js' 'npm run check-types' 'npm run debug:watch'",
        "debug:watch": "watchexec -e js -w dist 'node --inspect=0.0.0.0:9229 dist/src/main.js'",
        "dev": "watchexec -e ts -w src -r 'npx concurrently -n 'tsc' -p '{name}' -c 'bgBlue.black.bold' -i \"tsc --noEmit\" \"npm run build:fast && node --inspect=0.0.0.0:9229 dist/src/main.js --enable-source-maps\"'",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "migrate:create": "npm run typeorm -- migration:create -n",
        "migrate:up": "npm run typeorm -- migration:run",
        "migrate:down": "npm run typeorm -- migration:revert",
        "prebuild": "rimraf dist",
        "prod": "node dist/src/main.js",
        "seed:config": "ts-node ../../node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node ../../node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "npm run typeorm -- schema:drop",
        "schema:sync": "npm run typeorm -- schema:sync",
        "start": "node dist/src/main.js",
        "test": "cross-env NODE_ENV=test jest --config jest.config.ts ",
        "test:watch": "cross-env NODE_ENV=test jest --config jest.config.ts --watch",
        "test:cov": "cross-env NODE_ENV=test jest --config jest.config.ts --coverage",
        "test:debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.ts --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest.e2e-config.ts",
        "test:e2e-debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.e2e-config.ts --runInBand",
        "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js --config ./ormconfig.ts"
    },
    "dependencies": {
        "@nestjs/cli": "^8.1.1",
        "@nestjs/common": "^8.2.3",
        "@nestjs/core": "^8.2.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "^1.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.2.3",
        "@nestjs/platform-socket.io": "^8.2.3",
        "@nestjs/schedule": "^1.0.2",
        "@nestjs/swagger": "^5.1.5",
        "@nestjs/throttler": "^2.0.0",
        "@nestjs/typeorm": "^8.0.2",
        "@nestjs/websockets": "^8.2.3",
        "@schoolacious/types": "^0.1.0",
        "bcrypt": "^5.0.1",
        "cache-manager": "^3.4.4",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.14.0",
        "cookie-parser": "^1.4.5",
        "csurf": "^1.11.0",
        "date-fns": "^2.22.1",
        "dotenv": "^8.2.0",
        "helmet": "^4.4.1",
        "lodash": "^4.17.21",
        "nestjs-pino": "^2.3.1",
        "openvidu-node-client": "^2.20.2",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "pino-http": "^6.2.0",
        "pino-pretty": "^7.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.4.0",
        "socket.io": "^4.1.3",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.37",
        "uuid": "^8.3.2",
        "validator": "^13.6.0"
    },
    "devDependencies": {
        "@anatine/esbuild-decorators": "^0.2.11",
        "@nestjs/schematics": "^8.0.3",
        "@nestjs/testing": "^8.0.6",
        "@types/bcrypt": "^3.0.1",
        "@types/cache-manager-redis-store": "^2.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cron": "^1.7.3",
        "@types/csurf": "^1.11.1",
        "@types/express": "^4.17.13",
        "@types/faker": "^4.1.12",
        "@types/jest": "^27.0.1",
        "@types/lodash": "^4.14.169",
        "@types/node": "^16.11.10",
        "@types/passport-jwt": "^3.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/pg": "^8.6.1",
        "@types/pino": "^6.3.11",
        "@types/pino-http": "^5.7.0",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.0",
        "@types/validator": "^13.7.2",
        "cross-env": "^7.0.3",
        "esbuild": "^0.14.29",
        "eslint-plugin-import": "^2.24.0",
        "glob": "^7.1.7",
        "jest": "^27.2.0",
        "node-mocks-http": "^1.10.1",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typeorm-seeding": "^1.6.1"
    }
}
